{
  "name": "untyper",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@7.9.0",
  "description": "a simple typewriter for browser",
  "author": "Song wuk <1017301325@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/songwuk/untyper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/songwuk/untyper.git"
  },
  "bugs": "https://github.com/songwuk/untyper/issues",
  "keywords": [
    "TypeIt",
    "typewriter",
    "untyper",
    "typing"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "playground": "vite playground",
    "prepublishOnly": "nr build",
    "test": "vitest --config ./vitest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "parse5": "^7.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@antfu/ni": "^0.17.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.19.28",
    "@types/web-animations-js": "^2.2.16",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "pnpm": "^7.33.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "unbuild": "^0.8.11",
    "vite": "^3.2.10",
    "vitest": "^1.4.0"
  }
}
